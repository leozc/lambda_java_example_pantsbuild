
jar_library(name='dropwizard-latest',
            jars = [
              jar(org='io.dropwizard', name='dropwizard-core', rev='1.0.0')
            ])            
target(name='dropwizard', dependencies = [':dropwizard-latest'])

jar_library(name='dropwizard-swagger',
  jars=[
    jar(org='com.smoketurner', name='dropwizard-swagger', rev='1.0.0-1')
  ]
)

jar_library(name='dropwizard-auth',
            jars = [
              jar(org='io.dropwizard', name='dropwizard-auth', rev='1.0.0')
            ])

